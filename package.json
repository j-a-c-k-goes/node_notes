{
  "name": "node_notes",
  "version": "1.0.0",
  "description": "A Node.js based command-line application for compiling notes. ",
  "main": "./src/app.js",
  "scripts": {
		"add":"node src/app.js add --title --body",
		"list":"node src/app.js list",
		"read":"node src/app.js read --title",
		"remove":"node src/app.js remove --title",
		"burn":"node src/app.js burn",
		"hash":"node src/app.js hash",
		"backup":"node src/app.js backup",
		"oldest":"node src/app.js oldest",
		"newest":"node src/app.js newest",
		"help":"node src/app.js help",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jack",
  "license": "MIT",
  "dependencies": {
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "yargs": "^17.5.1"
  }
}
